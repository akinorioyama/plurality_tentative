---
layout: home
permalink: "/chapters/"
---

{%- from './_includes/macros/chapterversion-menu-item.njk' import chapterVersionMenuItem -%}

<table>
<tr>
<td valign="top">
<div id="version-menu">
  <ol id="version-ol">
      <li style="font-size:250%">
      Chapters
      </li>
<div>
    {% for version in collections.chapterversions |sortDataByChapteridSubidLanguageIso %}
{% if ((version.data.language.en) == 'english')%}
</div>
{% if ((version.data.chapterid.chapterid_subid) == '01') %}
    <div style="border:solid 10px #a9a9a9; border-radius: 8px; padding:10px; margin: 0 0 5% 0;">
        {% endif %}

{% if ((version.data.chapterid.chapterid_subid) == '03-00')
    or ((version.data.chapterid.chapterid_subid) == '02')
    or ((version.data.chapterid.chapterid_subid) == '04-00' )
    or ((version.data.chapterid.chapterid_subid) == '05-00' )
    or ((version.data.chapterid.chapterid_subid) == '06-00' )
    or ((version.data.chapterid.chapterid_subid) == '07-01' )
    %}
</div>
    <div style="border:solid 10px #a9a9a9; border-radius: 8px; padding:10px; margin: 0 0 5% 0;">
        {% endif %}
        {% endif %}

        {{ chapterVersionMenuItem(
          githubLink = version.data.githubLink,
          dayStarted = version.data.dateStartedString.day,
          monthStarted = version.data.dateStartedString.month,
          yearStarted = version.data.dateStartedString.year,
          language = version.data.language.en,
          contributeTranslated = version.data.translations.contribute,
          pluralityTranslated = version.data.translations.plurality,
          subtitleTranslated = version.data.translations.subtitle,
          pageTitle = version.data.title,
          pageUrl = version.url,
          chapterid_subid = version.data.chapterid.chapterid_subid)
        }}
    {% endfor %}
</table>
  </ol>
</div>
</td>
</tr>
</table>
